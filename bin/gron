#!/bin/bash

basedir="$(readlink -f "$(dirname "$0")")/.."

. "$basedir/lib/jq_stack.lib.sh"
. "$basedir/lib/jq_run.lib.sh"

. "$basedir/lib/jq.hide_last_array_index.lib.sh"
. "$basedir/lib/jq.json2flat.lib.sh"
. "$basedir/lib/jq.json2ndjson.lib.sh"
. "$basedir/lib/jq.ndjson2json.lib.sh"
. "$basedir/lib/jq.sortallarrays.lib.sh"
. "$basedir/lib/jq.gron.lib.sh"

jq_run json2flat hide_last_array_index sortallarrays json2ndjson gron
